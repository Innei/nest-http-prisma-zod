    "cache-manager-ioredis": "2.1.0",
    "chalk": "^5.3.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "consola": "*",
    "cron": "^2.3.1",
    "cross-env": "7.0.3",
    "dayjs": "1.11.9",
    "lodash": "*",
    "nanoid": "3.3.6",
    "nestjs-prisma": "0.22.0",
